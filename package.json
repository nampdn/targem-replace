{
  "name": "tauri-typescript-react-boilerplate",
  "version": "1.0.0",
  "description": "A Tauri Typescript React Boilerplate that works out of the box.",
  "keywords": [],
  "license": "MIT",
  "author": "Nate Adams",
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "prebuild": "rimraf ./dist",
    "build:fe": "webpack --production",
    "build:tauri": "tauri build",
    "build": "npm run build:fe && npm run build:tauri",
    "dev": "concurrently -p \"[{name}]\" -n \"front-end,tauri\" -c \"bgGreen.bold,bgCyan.bold\" \"npm run dev:fe\" \"RUST_DEBUG=1 tauri dev\"",
    "dev:win": "concurrently -p \"[{name}]\" -n \"front-end,tauri\" -c \"bgGreen.bold,bgCyan.bold\" \"npm run dev:fe\" \"set RUST_DEBUG=1 && tauri dev\"",
    "dev:fe": "webpack-dev-server --watch",
    "dev:tauri": "set RUST_DEBUG=1 && tauri dev"
  },
  "resolutions": {
    "minimist": "^1.2.2",
    "websocket-extensions": "0.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-transform-parameters": "^7.10.1",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@babel/preset-typescript": "^7.10.1",
    "@types/node": "^14.0.13",
    "@types/p-throttle": "2.1.0",
    "@types/react": "^16.9.35",
    "@types/subtitle": "2.0.2",
    "@types/webpack": "^4.41.17",
    "@types/webpack-dev-server": "^3.11.0",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.2.0",
    "core-js": "3.6.5",
    "css-loader": "^3.5.3",
    "glob": "^7.1.6",
    "html-webpack-plugin": "^4.3.0",
    "inline-source-cli": "^2.0.0",
    "patch-package": "6.2.2",
    "purgecss-webpack-plugin": "^2.2.0",
    "react": "^16.13.1",
    "react-app-polyfill": "^1.0.6",
    "react-dom": "^16.13.1",
    "rimraf": "^3.0.2",
    "style-loader": "^1.2.1",
    "tauri": "^0.6.2",
    "terser-webpack-plugin": "^3.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@material-ui/core": "4.10.2",
    "@material-ui/icons": "4.9.1",
    "axios": "^0.19.2",
    "google-translate-api-browser": "1.1.71",
    "graphql": "15.1.0",
    "graphql-request": "2.0.0",
    "graphql-tag": "2.10.3",
    "p-throttle": "3.1.0",
    "path-browserify": "1.0.1",
    "react-window": "1.8.5",
    "string-strip-html": "4.4.7",
    "subtitle": "2.0.3"
  }
}
